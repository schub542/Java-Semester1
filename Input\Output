import java.io.*;
import java.util.Scanner;

public class InpOut {
  //Throws IOException here because of createTriangles method
  public static void main(String[] args) throws IOException
  {
    // input output txt files
    final String INPUT = "Input.txt";
    final String OUTPUT = "triangleside.txt";
    
    boolean status = InpOut.structureTriangle(INPUT, OUTPUT);
    
    if (status)
    {
      System.out.println("OK");
    }
    else
    {
      System.out.println("Check your input or output files.");
    }
    
  }
  //throws clause to send exception that has not been delt with
  public static boolean structureTriangle(String inputFile, String outputFile) throws IOException
  {
    
    File inputFile1= new File("goodInput.txt");
    Scanner writtenFile= new Scanner(inputFile1);
    PrintWriter outputFile1= new PrintWriter ("triangles.txt");
    
//hasNext scanner class method to scan each data point in input file
    while(writtenFile.hasNext())
    {
      int number;  
      number = writtenFile.nextInt();
      
      //include cast operator to have compatiable types
      int a =(int) Math.pow(number,2.0);
      
      int number1;
      number1=writtenFile.nextInt();
      
      //include cast operator to have compatiable types
      int b= (int)Math.pow(number1,2.0);
      int x= a+b;
      
      //Math square root method
      double c=Math.sqrt(x);
      
      outputFile1.println(c);
      
    }
    
    //Close the files
    writtenFile.close();
    outputFile1.close();
    
    return true;
  }
}
